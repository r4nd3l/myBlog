<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>    function paros(arr){
    var ret = [];
    
    for(var i = 0; i &lt; arr.length; i++){
        var current = arr[i];
        if(current%2 === 0){
        ret.push(current);
        }
    }
    return ret;
    }

    console.log(paros([1,2,3,4,5,6,7,8]));
  
